<v-card>
  <v-card-title primary-title>
    <h2>Consultants</h2>
  </v-card-title>
  <v-card-actions>
    <v-btn primary href="<%= new_consultant_path %>">Create Consultant</v-btn>
  </v-card-actions>
</v-card>

<v-layout row mt-2>
  <v-flex xs12 sm6 offset-sm3>
    <v-card>
      <v-toolbar dark>
        <v-toolbar-title>Consultants</v-toolbar-title>
      </v-toolbar>
      <v-list two-line>
        <% @consultants.each do |consultant| %>
          <%= consultant.name %>
        <% end %>
      </v-list>
    </v-card>
  </v-flex>
</v-layout>
